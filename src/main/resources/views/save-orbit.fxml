    <TextField fx:id="newOrbit" layoutX="148.0" layoutY="98.0" prefHeight="26.0" prefWidth="338.0" />
    <TextField fx:id="newSpeed" layoutX="148.0" layoutY="128.0" prefHeight="26.0" prefWidth="338.0" />
    <TextField fx:id="newAlfa" layoutX="148.0" layoutY="158.0" prefHeight="26.0" prefWidth="338.0" />

    <Label fx:id="newOrbitLog" layoutX="66.0" layoutY="195.0" />

    <Button fx:id="newOrbitSaveButton" layoutX="187.0" layoutY="220.0" mnemonicParsing="false" onAction="#saveNewOrbit" text="Сохранить" />
