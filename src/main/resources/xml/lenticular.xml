<form name="ЧЕЧЕВИЦА">
    <area>(PI * pow(radius, 2) - pow(length, 2)/4) * abs(cos(alfa)) + (PI * sqrt(pow(radius, 2) - pow(length, 2)/4) * (radius - length/2)) * abs(sin(alfa))</area>
    <midsectionArea>(PI * pow(radius, 2) - pow(length, 2)/4)</midsectionArea>
    <centrePressureX>cubesatSize.length + radius - length / 2</centrePressureX>
    <centrePressureY>cubesatSize.width / 2</centrePressureY>
    <centrePressureZ>cubesatSize.height / 2</centrePressureZ>
    <centreMassX>cubesatSize.length + radius - length / 2</centreMassX>
    <centreMassY>cubesatSize.width / 2</centreMassY>
    <centreMassZ>cubesatSize.height / 2</centreMassZ>
    <shellNtu>4 * PI * radius * (radius - sqrt(pow(radius, 2) - pow(length, 2) / 4))</shellNtu>
</form>